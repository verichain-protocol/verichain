# DFX Canister Configuration
DFX_VERSION='0.27.0'
DFX_NETWORK='local'
CANISTER_ID_LOGIC_CANISTER='your-logic-canister-id'
CANISTER_ID_INTERNET_IDENTITY='your-ii-canister-id'
CANISTER_ID_FRONTEND='your-frontend-canister-id'
CANISTER_ID_AI_CANISTER='your-ai-canister-id'
CANISTER_ID='your-main-canister-id'
CANISTER_CANDID_PATH='/app/src/ai_canister/src/ai_canister.did'

# Docker & Build Environment
RUST_BACKTRACE=1

# Set build jobs according to available CPU cores
CARGO_BUILD_JOBS=4
