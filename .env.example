# VeriChain Environment Configuration
# Copy this file to .env and customize for your environment

# DFX & Internet Computer
DFX_VERSION=0.27.0
DFX_NETWORK=local

# Canister IDs (auto-generated by dfx deploy)
# CANISTER_ID_LOGIC_CANISTER=your-logic-canister-id
# CANISTER_ID_INTERNET_IDENTITY=your-ii-canister-id
# CANISTER_ID_FRONTEND=your-frontend-canister-id
# CANISTER_ID_AI_CANISTER=your-ai-canister-id

# Build Environment
RUST_BACKTRACE=1
NODE_ENV=development

# AI Model Configuration
AI_MODEL_CHUNK_SIZE_MB=15
MAX_MODEL_SIZE_MB=100

# Performance
MAX_CYCLES_PER_CALL=1000000000

# Development
DEBUG=true
LOG_LEVEL=info