# VeriChain Environment Configuration
# Copy this file to .env and customize as needed

# Model Configuration
MODEL_CHUNK_SIZE_MB=0.8
MODEL_DOWNLOAD_URL=https://huggingface.co/einrafh/verichain-deepfake-models/resolve/main/models/onnx/verichain-model.onnx

# Network Configuration
DEPLOY_NETWORK=local
DFX_NETWORK=local

# Frontend Configuration
FRONTEND_PORT=3001
FRONTEND_HOST=localhost

# AI Canister Configuration
AI_CANISTER_MEMORY_MB=512
AI_CANISTER_CYCLES=2000000000000

# Development Settings
NODE_ENV=development
LOG_LEVEL=info

# Testing Configuration
TEST_TIMEOUT=300
TEST_IMAGE_SIZE=1024x1024

# Performance Tuning
BATCH_SIZE=10
MAX_CONCURRENT_REQUESTS=100
INFERENCE_TIMEOUT=30