# VeriChain Environment Configuration Template
# Copy this file to .env and customize with your actual values

# DFX CANISTER ENVIRONMENT VARIABLES
# These values are auto-generated by DFX when you run 'dfx deploy'
DFX_VERSION='0.27.0'
DFX_NETWORK='local'
CANISTER_ID_AI_CANISTER='your-ai-canister-id'
CANISTER_ID_FRONTEND='your-frontend-canister-id'
CANISTER_ID_INTERNET_IDENTITY='your-internet-identity-canister-id'
CANISTER_ID_LOGIC_CANISTER='your-logic-canister-id'
CANISTER_ID='your-main-canister-id'
CANISTER_CANDID_PATH='/your/project/path/.dfx/local/canisters/logic_canister/logic_canister.did'
# END DFX CANISTER ENVIRONMENT VARIABLES

# VeriChain Model Configuration
MODEL_CHUNK_SIZE_MB=0.8
MODEL_DOWNLOAD_URL='https://huggingface.co/einrafh/verichain-deepfake-models/resolve/main/models/onnx/verichain-model.onnx'

# Network Configuration
DEPLOY_NETWORK='local'

# Frontend Configuration
FRONTEND_PORT=3000
FRONTEND_HOST='localhost'

# AI Canister Configuration
AI_CANISTER_MEMORY_MB=512
AI_CANISTER_CYCLES=2000000000000

# Development Settings
NODE_ENV='development'
LOG_LEVEL='info'

# Testing Configuration
TEST_TIMEOUT=300
TEST_IMAGE_SIZE='1024x1024'

# Performance Tuning
BATCH_SIZE=10
MAX_CONCURRENT_REQUESTS=100
INFERENCE_TIMEOUT=30